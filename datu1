#include <bits/stdc++.h>

using namespace std;
int n,m,i,j;
int a[100][30];
int f[100][30];
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    cin>>m>>n;
    for(i=1;i<=m;i++){for(j=1;j<=n;j++) {cin>>a[i][j];} }
    for(i=1;i<=m;i++)
    {
        for(j=1;j<=n;j++)
        {
            int res=INT_MIN;
            for(int z=0;z<=i;z++)
            {
                res=max(res,a[z][j]+f[i-z][j-1]);
            }
            f[i][j]=max(res,f[i][j-1]);
           /// cout<<f[i][j]<<" ";
        }
        ///cout<<'\n';
    }
    cout<<f[m][n];

}
